{
  "builds": [
    {
      "src": "src/main.tsx",
      "use": "@vercel/node"
    },
    {
      "src": "tsconfig.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/main.tsx"
    }
  ]
}
